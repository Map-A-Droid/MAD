          }
        },
        {
          "name": "screenshot_y_offset",
          "settings": {
            "type": "text",
            "require": "false",
            "description": "Adjust the y-axis click offset on phones with softbars and/or black upper bars. (+ down - up / Default: 0)",
            "expected": "int"
          }
        },
        {
          "name": "screenshot_type",
