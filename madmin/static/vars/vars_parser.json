        },
        {
          "name": "restart_thresh",
          "settings": {
            "type": "text",
            "require": "false",
            "description": "Restart Pogo after reaching MITM Timeout N times. (Default: 5)"
            ,
            "expected": "int"
          }
        },
        {
          "name": "post_screenshot_delay",
          "settings": {
            "type": "text",
            "require": "false",
