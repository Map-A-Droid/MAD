{
  "devices": [
    {
      "name": "devices",
      "fields": [
        {
          "name": "origin",
          "settings": {
            "type": "text",
            "require": "true",
            "empty": "null",
            "description": "Name of device (from RGC settings)",
            "lockonedit": "true",
            "expected": "str"
          }
        },
        {
          "name": "walker",
          "settings": {
            "type": "walkerselect",
            "require": "true",
            "empty": null,
